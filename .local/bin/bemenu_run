#!/usr/bin/env sh

 j4-dmenu-desktop --no-generic --dmenu="((stest -flx $(echo $PATH | tr : ' ') | sort -u);cat) | bemenu -H 30 -i --fn 'JetBrainsMono Bold 10' --nb='#006060' --sb='#338080' --tb='#006060' --tf='#bbbbbb' --fb='#006060' --ff='#bbbbbb' --tb='#338080' --tf='#ffffff' --hb='#338080' --hf='#ffffff' -p ''"
